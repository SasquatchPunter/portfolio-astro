{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts": [
        "src/layouts"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@tina": ["tina"],
      "@tina/*": ["tina/*"],
      "@lib": ["src/lib"],
      "@lib/*": ["src/lib/*"],
      "@icons": ["src/icons"],
      "@icons/*": ["src/icons/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}