---
import { StageBanner } from "@components/astro";
import BaseBody from "./BaseBody.astro";
import BaseHead from "./BaseHead.astro";

export interface Props {
  /** Page Title */
  title: string;
}

const { title } = Astro.props;
---

<html lang="en">
  <BaseHead {title}>
    <slot name="head" />
  </BaseHead>

  <BaseBody>
    <StageBanner />
    <slot />
  </BaseBody>
</html>
